{
    "id": "stack-opacity",
    "name": "Stack Opacity",
    "version": "1.0.0",
    "icons": [
        {
            "width": 23,
            "height": 23,
            "path": "icons/light@1x.png",
            "scale": [1, 1.25],
            "theme": ["light", "lightest"]
        },
        {
            "width": 46,
            "height": 46,
            "path": "icons/light@2x.png",
            "scale": [1.5, 2],
            "theme": ["light", "lightest"]
        },
        {
            "width": 23,
            "height": 23,
            "path": "icons/dark@1x.png",
            "scale": [1, 1.25],
            "theme": ["dark", "darkest"]
        },
        {
            "width": 46,
            "height": 46,
            "path": "icons/dark@2x.png",
            "scale": [1.5, 2],
            "theme": ["dark", "darkest"]
        }
    ],
    "main": "index.html",
    "host": [
        {
            "app": "PS",
            "minVersion": "23.3.0"
        }
    ],
    "manifestVersion": 5,
    "entrypoints": [
        {
            "type": "panel",
            "id": "mainPanel",
            "label": {
                "default": "Stack Opacity"
            },
            "minimumSize": {
                "width": 350,
                "height": 300
            },
            "maximumSize":{
                "width": 800,
                "height": 700
            },
            "preferredFloatingSize": { 
                "width": 350,
                "height": 400
            },
            "preferredDockedSize": {
                "width": 500,
                "height": 300
            }
        }
    ]
}